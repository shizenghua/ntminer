<blankWindow:BlankWindow x:Class="NTMiner.Views.MinerClientsWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:blankWindow="http://NTMiner.com"
		xmlns:sys="clr-namespace:System;assembly=mscorlib"
		xmlns:controls="clr-namespace:NTMiner.Controls;assembly=NTMinerWpf"
		xmlns:local="clr-namespace:NTMiner.Views"
		xmlns:view="clr-namespace:NTMiner.Views"
		xmlns:uc="clr-namespace:NTMiner.Views.Ucs"
		xmlns:vm="clr-namespace:NTMiner.Vms;assembly=AppModels"
		xmlns:app="clr-namespace:NTMiner;assembly=AppModels"
		Background="{StaticResource ThemeBackground}"
		WindowState="Maximized"
		DataContext="{x:Static vm:MinerClientsWindowViewModel.Instance}"
		WindowStartupLocation="CenterScreen"
		MouseDown="MetroWindow_MouseDown"
		Title="矿机列表"
		mc:Ignorable="d" d:DesignHeight="1000" d:DesignWidth="3600">
	<blankWindow:BlankWindow.Resources>
		<ResourceDictionary>
			<Style x:Key="LblTb" TargetType="TextBlock" BasedOn="{StaticResource LblTbBase}">
			</Style>
		</ResourceDictionary>
	</blankWindow:BlankWindow.Resources>
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"></RowDefinition>
			<RowDefinition Height="Auto"></RowDefinition>
			<RowDefinition Height="*"></RowDefinition>
			<RowDefinition Height="Auto"></RowDefinition>
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="220"></ColumnDefinition>
			<ColumnDefinition Width="*"></ColumnDefinition>
		</Grid.ColumnDefinitions>
		<!--上标题栏-->
		<Grid Grid.Column="0" Grid.ColumnSpan="2">
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="*"></ColumnDefinition>
				<ColumnDefinition Width="Auto"></ColumnDefinition>
			</Grid.ColumnDefinitions>
			<WrapPanel Grid.Column="0" Margin="4" VerticalAlignment="Center">
				<Path
					Width="12"
					Height="12"
					Data="{StaticResource Icon_Miner}"
					Fill="{StaticResource IconFillColor}"
					Stretch="Fill" />
				<TextBlock
					VerticalAlignment="Center"
					Margin="4 0 8 0"
					FontSize="14"
					Foreground="{StaticResource ThemeForeground}"
					Text="开源矿工群控 - 矿机列表">
				</TextBlock>
				<uc:MainBar></uc:MainBar>
			</WrapPanel>
			<WrapPanel Grid.Column="1">
				<controls:KbButton 
					ToolTip="收益计算器"
					Command="{x:Static app:AppStatic.ShowCalc}" 
					Background="Transparent" 
					BorderThickness="0"
					Margin="0 0 12 0"
					CornerRadius="2">
					<Path 
						Width="18"
						Height="18"
						Data="{StaticResource Icon_Calc}"
						Fill="{StaticResource ThemeForeground}"
						Stretch="Fill" />
				</controls:KbButton>
				<controls:KbButton 
					Margin="0 0 12 0"
					ToolTip="算力图"
					Command="{x:Static app:AppStatic.ShowChartsWindow}" Background="Transparent" BorderThickness="0"
					CornerRadius="2">
					<Image Width="18" Height="18" Source="/NTMinerWpf;component/Styles/Images/cc32.png">
					</Image>
				</controls:KbButton>
				<uc:MainMenu Background="Transparent">
				</uc:MainMenu>
				<controls:KbSystemMinButton
					Height="30"
					KbSystemButtonSize="40" />
				<controls:KbSystemMaxButton
					Height="30"
					KbSystemButtonSize="40" />
				<controls:KbSystemCloseButton
					Height="30"
					KbSystemButtonSize="40" />
			</WrapPanel>
		</Grid>
		<WrapPanel Grid.Row="1" Grid.Column="2" Background="#EEEEF2">
			<Menu>
				<MenuItem Header="开挖" Padding="4" Margin="0 0 6 0" Command="{Binding StartMine}">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_Start}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
				<MenuItem Header="停挖" Padding="4" Margin="0 0 6 0" Command="{Binding StopMine}">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_Stop}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
				<MenuItem Header="刷新" x:Name="MenuItemRefresh" Padding="4" Margin="0 0 6 0" Command="{Binding RefreshMinerClients}">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_Refresh}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
				<MenuItem Header="切换作业" ToolTip="为选中的矿机指定作业" x:Name="MenuItemMineWork" IsEnabled="{Binding ElementName=MenuItemRefresh, Path=IsEnabled}" Padding="4" Margin="0 0 6 0" Click="MenuItemWork_Click">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_MineWork}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
				<MenuItem Header="编辑作业" Padding="4" Margin="0 0 6 0" Command="{Binding EditMineWork}">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_Edit}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
				<MenuItem Header="分组" ToolTip="将选中的矿机放入分组" x:Name="MenuItemMinerGroup" IsEnabled="{Binding ElementName=MenuItemRefresh, Path=IsEnabled}" Padding="4" Margin="0 0 6 0" Click="MenuItemGroup_Click">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_MinerGroup}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
				<MenuItem Command="{Binding OneKeyMinerNames}" Padding="4" Margin="0 0 6 0" Header="群控矿工名">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_MinerName}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
				<MenuItem Command="{Binding OneKeySetting}" Padding="4" Margin="0 0 6 0" Header="设置">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_Setting}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
				<MenuItem Header="超频" Padding="4" Margin="0 0 6 0" Command="{Binding OneKeyOverClock}">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_OverClock}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
				<MenuItem 
					x:Name="MenuItemUpgrade" 
					Header="升级" Padding="4" Margin="0 0 6 0" 
					IsEnabled="{Binding ElementName=MenuItemRefresh, Path=IsEnabled}" 
					Click="MenuItemUpgrade_Click">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_Update}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
				<MenuItem Header="重启" Padding="4" Margin="0 0 6 0" Command="{Binding RestartNTMiner}">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_Restart}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
				<MenuItem Header="重启电脑" Padding="4" Margin="0 0 6 0" Command="{Binding RestartWindows}">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_Restart2}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
				<MenuItem Header="关机" ToolTip="关闭电脑" Padding="4" Margin="0 0 6 0" Command="{Binding ShutdownWindows}">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_Shutdown}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
				<MenuItem Command="{Binding OneKeyWindowsLoginName}" Padding="4" Margin="0 0 6 0" Header="一键远程桌面用户名">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_RemoteDesktop}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
				<MenuItem Command="{Binding OneKeyWindowsLoginPassword}" Padding="4" Margin="0 0 6 0" Header="一键远程桌面密码">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_RemoteDesktop}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
				<MenuItem Header="添加矿机" Padding="4" Margin="0 0 6 0" Command="{Binding AddMinerClient}">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_Add}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
				<MenuItem Header="删除矿机" Padding="4" Margin="0 0 6 0" Command="{Binding RemoveMinerClients}">
					<MenuItem.Icon>
						<Path
							Width="16"
							Height="16"
							Data="{StaticResource Icon_Delete}"
							Fill="{StaticResource IconFillColor}"
							Stretch="Fill" />
					</MenuItem.Icon>
				</MenuItem>
			</Menu>
		</WrapPanel>
		<StackPanel Grid.Row="1" Grid.Column="0" Grid.RowSpan="2" Background="#F5F5F5">
			<ListBox 
				Background="Transparent"
				ItemsSource="{Binding MineCoinVms.PleaseSelect}"
				SelectedItem="{Binding CoinVm}"
				ScrollViewer.HorizontalScrollBarVisibility="Disabled"
				ScrollViewer.VerticalScrollBarVisibility="Disabled"
				ItemContainerStyle="{StaticResource ListBoxBarItem}"
				BorderThickness="0">
				<ListBox.ItemsPanel>
					<ItemsPanelTemplate>
						<WrapPanel>
						</WrapPanel>
					</ItemsPanelTemplate>
				</ListBox.ItemsPanel>
				<ListBox.ItemTemplate>
					<DataTemplate>
						<TextBlock Padding="10 2" Text="{Binding Code}"></TextBlock>
					</DataTemplate>
				</ListBox.ItemTemplate>
			</ListBox>
			<GroupBox>
				<GroupBox.Header>
					<WrapPanel>
						<TextBlock VerticalAlignment="Center">作业</TextBlock>
						<controls:KbButton  
							Command="{Binding MineWorkVms.Add}" 
							Background="Transparent" 
							BorderThickness="0"
							Margin="2 0"
							CornerRadius="2">
							<WrapPanel>
								<Path
									Width="18"
									Height="18"
									ToolTip="新建作业"
									Data="{StaticResource Icon_Add}"
									Fill="{StaticResource IconFillColor}"
									Stretch="Fill" />
							</WrapPanel>
						</controls:KbButton>
						<controls:KbButton  
							Command="{Binding SelectedMineWork.Edit}" 
							Background="Transparent" 
							BorderThickness="0"
							Margin="2 0"
							CornerRadius="2">
							<WrapPanel>
								<Path
									Width="18"
									Height="18"
									ToolTip="编辑作业"
									Data="{StaticResource Icon_Edit}"
									Fill="{StaticResource IconFillColor}"
									Stretch="Fill" />
							</WrapPanel>
						</controls:KbButton>
						<controls:KbButton  
							Command="{Binding SelectedMineWork.Remove}" 
							Background="Transparent" 
							BorderThickness="0"
							Margin="2 0"
							CornerRadius="2">
							<WrapPanel>
								<Path
									Width="18"
									Height="18"
									ToolTip="删除作业"
									Data="{StaticResource Icon_Delete}"
									Fill="{StaticResource IconFillColor}"
									Stretch="Fill" />
							</WrapPanel>
						</controls:KbButton>
					</WrapPanel>
				</GroupBox.Header>
				<ListBox 
					Background="Transparent"
					ItemsSource="{Binding MineWorkVms.MineWorkVmItems}"
					SelectedItem="{Binding SelectedMineWork}"
					ScrollViewer.HorizontalScrollBarVisibility="Disabled"
					ScrollViewer.VerticalScrollBarVisibility="Disabled"
					ItemContainerStyle="{StaticResource ListBoxBarItem}"
					BorderThickness="0">
					<ListBox.ItemsPanel>
						<ItemsPanelTemplate>
							<WrapPanel>
							</WrapPanel>
						</ItemsPanelTemplate>
					</ListBox.ItemsPanel>
					<ListBox.ItemTemplate>
						<DataTemplate>
							<TextBlock Padding="10 2" Text="{Binding Name}" ToolTip="{Binding Description}"></TextBlock>
						</DataTemplate>
					</ListBox.ItemTemplate>
				</ListBox>
			</GroupBox>
			<StackPanel Visibility="{Binding IsMineWorkSelected, Converter={StaticResource BoolToVisibilityInvertConverter}}">
				<GroupBox>
					<GroupBox.Header>
						<WrapPanel>
							<TextBlock VerticalAlignment="Center">分组</TextBlock>
							<controls:KbButton  
								Command="{Binding MinerGroupVms.Add}" 
								Background="Transparent" 
								ToolTip="新建组"
								BorderThickness="0"
								Margin="2 0"
								CornerRadius="2">
								<WrapPanel>
									<Path
										Width="18"
										Height="18"
										Data="{StaticResource Icon_Add}"
										Fill="{StaticResource IconFillColor}"
										Stretch="Fill" />
								</WrapPanel>
							</controls:KbButton>
							<controls:KbButton  
								Command="{Binding SelectedMinerGroup.Edit}" 
								Background="Transparent" 
								BorderThickness="0"
								Margin="2 0"
								CornerRadius="2">
								<WrapPanel>
									<Path
										Width="18"
										Height="18"
										ToolTip="编辑组"
										Data="{StaticResource Icon_Edit}"
										Fill="{StaticResource IconFillColor}"
										Stretch="Fill" />
								</WrapPanel>
							</controls:KbButton>
							<controls:KbButton  
									Command="{Binding SelectedMinerGroup.Remove}" 
									Background="Transparent" 
									BorderThickness="0"
									Margin="2 0"
									CornerRadius="2">
								<WrapPanel>
									<Path
										Width="18"
										Height="18"
										ToolTip="删除组"
										Data="{StaticResource Icon_Delete}"
										Fill="{StaticResource IconFillColor}"
										Stretch="Fill" />
								</WrapPanel>
							</controls:KbButton>
						</WrapPanel>
					</GroupBox.Header>
					<ListBox 
						Background="Transparent"				
						ItemsSource="{Binding MinerGroupVms.MinerGroupItems}"
						SelectedItem="{Binding SelectedMinerGroup}"
						ScrollViewer.HorizontalScrollBarVisibility="Disabled"
						ScrollViewer.VerticalScrollBarVisibility="Disabled"
						ItemContainerStyle="{StaticResource ListBoxBarItem}"
						BorderThickness="0">
						<ListBox.ItemsPanel>
							<ItemsPanelTemplate>
								<WrapPanel>
								</WrapPanel>
							</ItemsPanelTemplate>
						</ListBox.ItemsPanel>
						<ListBox.ItemTemplate>
							<DataTemplate>
								<TextBlock Padding="10 2" Text="{Binding Name}" ToolTip="{Binding Description}"></TextBlock>
							</DataTemplate>
						</ListBox.ItemTemplate>
					</ListBox>
				</GroupBox>
				<GroupBox Header="状态">
					<ListBox 
						Background="Transparent"
						ItemsSource="{x:Static app:AppStatic.MineStatusEnumItems}"
						SelectedItem="{Binding MineStatusEnumItem}"
						ScrollViewer.HorizontalScrollBarVisibility="Disabled"
						ScrollViewer.VerticalScrollBarVisibility="Disabled"
						ItemContainerStyle="{StaticResource ListBoxBarItem}"
						BorderThickness="0">
						<ListBox.ItemsPanel>
							<ItemsPanelTemplate>
								<WrapPanel>
								</WrapPanel>
							</ItemsPanelTemplate>
						</ListBox.ItemsPanel>
						<ListBox.ItemTemplate>
							<DataTemplate>
								<TextBlock Padding="10 2" Text="{Binding Description}"></TextBlock>
							</DataTemplate>
						</ListBox.ItemTemplate>
					</ListBox>
				</GroupBox>
				<GroupBox Header="矿池">
					<StackPanel>
						<TextBox HorizontalAlignment="Left" VerticalContentAlignment="Center" Text="{Binding Pool}" Width="208" Background="White"></TextBox>
						<ListBox 
							Background="Transparent"
							ItemsSource="{Binding CoinVm.OptionPools}"
							SelectedItem="{Binding PoolVm}"
							ScrollViewer.HorizontalScrollBarVisibility="Disabled"
							ScrollViewer.VerticalScrollBarVisibility="Disabled"
							ItemContainerStyle="{StaticResource ListBoxBarItem}"
							BorderThickness="0">
							<ListBox.ItemsPanel>
								<ItemsPanelTemplate>
									<WrapPanel>
									</WrapPanel>
								</ItemsPanelTemplate>
							</ListBox.ItemsPanel>
							<ListBox.ItemTemplate>
								<DataTemplate>
									<TextBlock Padding="10 2" Text="{Binding Name}"></TextBlock>
								</DataTemplate>
							</ListBox.ItemTemplate>
						</ListBox>
					</StackPanel>
				</GroupBox>
				<GroupBox Header="钱包">
					<TextBox HorizontalAlignment="Left" VerticalContentAlignment="Center" Text="{Binding Wallet}" Width="208" Background="White"></TextBox>
				</GroupBox>
				<WrapPanel>
					<TextBlock Style="{StaticResource LblTb}" Width="50">IP</TextBlock>
					<TextBox VerticalContentAlignment="Center" Width="150" Text="{Binding MinerIp, ValidatesOnExceptions=True}" Background="White"></TextBox>
				</WrapPanel>
				<WrapPanel>
					<TextBlock Style="{StaticResource LblTb}" Width="50">矿机名</TextBlock>
					<TextBox VerticalContentAlignment="Center" Width="150" Text="{Binding MinerName}" Background="White"></TextBox>
				</WrapPanel>
				<WrapPanel>
					<TextBlock Style="{StaticResource LblTb}" Width="50">内核</TextBlock>
					<TextBox VerticalContentAlignment="Center" ToolTip="内核名以此开头的内核" Width="150" Text="{Binding Kernel}" Background="White"></TextBox>
				</WrapPanel>
				<WrapPanel>
					<TextBlock Style="{StaticResource LblTb}" Width="50">版本</TextBlock>
					<TextBox VerticalContentAlignment="Center" ToolTip="客户端版本号以此开头的客户端" Width="150" Text="{Binding Version}" Background="White"></TextBox>
				</WrapPanel>
			</StackPanel>
		</StackPanel>
		<Popup x:Name="PopMineWork" Grid.Row="2" Grid.Column="1" PopupAnimation="Slide" PlacementTarget="{Binding ElementName=MenuItemMineWork}" Placement="Bottom" StaysOpen="False">
			<Border Padding="4" MinWidth="100" Background="#EEEEF2" BorderBrush="{StaticResource IconFillColor}" BorderThickness="1">
				<StackPanel>
					<controls:KbButton  
						HorizontalAlignment="Left"
						Command="{Binding MineWorkVms.Add}" 
						Background="Transparent" 
						BorderThickness="0"
						Margin="2 0"
						CornerRadius="2">
						<WrapPanel>
							<Path
								Width="18"
								Height="18"
								ToolTip="新建作业"
								Data="{StaticResource Icon_Add}"
								Fill="{StaticResource IconFillColor}"
								Stretch="Fill" />
						</WrapPanel>
					</controls:KbButton>
					<ItemsControl 
						ItemsSource="{Binding MineWorkVms.MineWorkVmItems}"
						ScrollViewer.HorizontalScrollBarVisibility="Disabled"
						ScrollViewer.VerticalScrollBarVisibility="Disabled">
						<ItemsControl.Resources>
							<controls:BindingProxy x:Key="proxy" Data="{Binding}" />
						</ItemsControl.Resources>
						<ItemsControl.ItemsPanel>
							<ItemsPanelTemplate>
								<StackPanel>
								</StackPanel>
							</ItemsPanelTemplate>
						</ItemsControl.ItemsPanel>
						<ItemsControl.ItemTemplate>
							<DataTemplate>
								<Button 
									Click="PopupButton_Click"
									BorderBrush="{StaticResource LineColor}"
									Margin="4"
									Padding="10 2" 
									Content="{Binding Name}" 
									ToolTip="{Binding Description}"
									Command="{Binding Data.OneKeyWork,Source={StaticResource proxy}}"
									CommandParameter="{Binding}"></Button>
							</DataTemplate>
						</ItemsControl.ItemTemplate>
					</ItemsControl>
				</StackPanel>
			</Border>
		</Popup>
		<Popup x:Name="PopMinerGroup" Grid.Row="2" Grid.Column="1" PopupAnimation="Slide" PlacementTarget="{Binding ElementName=MenuItemMinerGroup}" Placement="Bottom" StaysOpen="False">
			<Border Padding="4" MinWidth="100" Background="#EEEEF2" BorderBrush="{StaticResource IconFillColor}" BorderThickness="1">
				<StackPanel>
					<controls:KbButton  
						HorizontalAlignment="Left"
						Command="{Binding MinerGroupVms.Add}" 
						Background="Transparent" 
						ToolTip="新建组"
						BorderThickness="0"
						Margin="2 0"
						CornerRadius="2">
						<WrapPanel>
							<Path
								Width="18"
								Height="18"
								Data="{StaticResource Icon_Add}"
								Fill="{StaticResource IconFillColor}"
								Stretch="Fill" />
						</WrapPanel>
					</controls:KbButton>
					<ItemsControl 
						ItemsSource="{Binding MinerGroupVms.MinerGroupItems}"
						ScrollViewer.HorizontalScrollBarVisibility="Disabled"
						ScrollViewer.VerticalScrollBarVisibility="Disabled">
						<ItemsControl.Resources>
							<controls:BindingProxy x:Key="proxy" Data="{Binding}" />
						</ItemsControl.Resources>
						<ItemsControl.ItemsPanel>
							<ItemsPanelTemplate>
								<StackPanel>
								</StackPanel>
							</ItemsPanelTemplate>
						</ItemsControl.ItemsPanel>
						<ItemsControl.ItemTemplate>
							<DataTemplate>
								<Button 
									Click="PopupButton_Click"
									BorderBrush="{StaticResource LineColor}"
									Margin="4"
									Padding="10 2" 
									Content="{Binding Name}" 
									ToolTip="{Binding Description}" 
									Command="{Binding Data.OneKeyGroup,Source={StaticResource proxy}}"
									CommandParameter="{Binding}"></Button>
							</DataTemplate>
						</ItemsControl.ItemTemplate>
					</ItemsControl>
				</StackPanel>
			</Border>
		</Popup>
		<Popup x:Name="PopUpgrade" Grid.Row="2" Grid.Column="1" PopupAnimation="Slide" PlacementTarget="{Binding ElementName=MenuItemUpgrade}" Placement="Bottom" StaysOpen="False">
			<Border Padding="4" MinWidth="100" Background="#EEEEF2" BorderBrush="{StaticResource IconFillColor}" BorderThickness="1">
				<StackPanel>
					<TextBlock>升级挖矿端</TextBlock>
					<ItemsControl 
						ItemsSource="{Binding NTMinerFileList}"
						ScrollViewer.HorizontalScrollBarVisibility="Disabled"
						ScrollViewer.VerticalScrollBarVisibility="Disabled">
						<ItemsControl.Resources>
							<controls:BindingProxy x:Key="proxy" Data="{Binding}" />
						</ItemsControl.Resources>
						<ItemsControl.ItemsPanel>
							<ItemsPanelTemplate>
								<StackPanel>
								</StackPanel>
							</ItemsPanelTemplate>
						</ItemsControl.ItemsPanel>
						<ItemsControl.ItemTemplate>
							<DataTemplate>
								<Button 
									Click="PopupButton_Click"
									BorderBrush="{StaticResource LineColor}"
									Margin="4"
									Padding="10 2" 
									Content="{Binding Version}" 
									Command="{Binding Data.OneKeyUpgrade,Source={StaticResource proxy}}"
									CommandParameter="{Binding}"></Button>
							</DataTemplate>
						</ItemsControl.ItemTemplate>
					</ItemsControl>
				</StackPanel>
			</Border>
		</Popup>
		<GridSplitter Grid.Row="2" Grid.Column="0" BorderThickness="0 0 1 0" BorderBrush="{StaticResource LineColor}" VerticalAlignment="Stretch" HorizontalAlignment="Right" Width="6" Background="Transparent"></GridSplitter>
		<Grid Grid.Row="2" Grid.Column="1" Background="{StaticResource MinerProfileBackground}">
			<Grid.RowDefinitions>
				<RowDefinition Height="*"></RowDefinition>
				<RowDefinition Height="470"></RowDefinition>
			</Grid.RowDefinitions>
			<DataGrid 
				Grid.Row="0"
				Margin="0 0 0 28"
				BorderThickness="0"
				IsReadOnly="True"
